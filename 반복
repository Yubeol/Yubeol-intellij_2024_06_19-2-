package org.example;
// 문제 : 아래가 실행되도록 해주세요.

class Main {
    public static void main(String[] args) {
        for(int i = 0; i < 10; i++){
            System.out.println("i : " +  i );
            for (int j = 0; i < 10; j++){
                System.out.println("j : " + j);
            }
        }
    }
}
